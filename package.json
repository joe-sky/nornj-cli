{
  "name": "nornj-cli",
  "version": "0.5.0-beta.2",
  "description": "The CLI for scaffolding NornJ template projects, and do more things",
  "bin": {
    "nj": "bin/nj"
  },
  "files": [
    "bin/",
    "docs/learningGuide.md",
    "lib/",
    "templates/sourceFiles/",
    "templates.config.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "tar": "node lib/tar",
    "lint": "eslint lib",
    "format": "prettier-eslint --write lib/**/*.js",
    "format-css-react-mst": "prettier-stylelint --write templates/react-mst/src/**/*.{css,less,scss}",
    "format-js-react-mst": "prettier-eslint --write templates/react-mst/**/*.{js,jsx}",
    "format-react-mst": "npm run format-css-react-mst && npm run format-js-react-mst",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/nornj-cli.git"
  },
  "keywords": [
    "cli",
    "react"
  ],
  "author": "Joe_Sky <hitomi_doax@sina.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-typescript": "^7.3.3",
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "download-git-repo": "^1.0.1",
    "git-exec": "^0.2.1",
    "global-modules": "^2.0.0",
    "inquirer": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "nornj": "^5.0.0-rc.22",
    "ora": "^1.3.0",
    "prettier-eslint": "^9.0.0",
    "process.args": "0.0.6",
    "rimraf": "^2.6.3",
    "targz": "^1.0.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^8.2.3",
    "eslint": "^6.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "prettier": "^1.13.7",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "typescript": "^3.5.2",
    "vuepress": "^1.0.2"
  }
}
