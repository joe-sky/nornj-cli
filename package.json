{
  "name": "nornj-cli",
  "version": "0.3.2-alpha.2",
  "description": "The CLI for scaffolding NornJ template projects, and do more things",
  "bin": {
    "nj": "bin/nj"
  },
  "files": [
    "bin/",
    "docs/",
    "lib/",
    "templates/sourceFiles/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "tar": "node lib/tar",
    "format": "prettier-eslint --write lib/**/*.js",
    "format-css-react-mst-universal": "prettier-stylelint --write templates/react-mst-universal/src/**/*.{css,less,scss}",
    "format-js-react-mst-universal": "prettier-eslint --write templates/react-mst-universal/**/*.{js,jsx}",
    "format-react-mst-universal": "npm run format-css-react-mst-universal && npm run format-js-react-mst-universal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/nornj-cli.git"
  },
  "keywords": [
    "cli",
    "react"
  ],
  "author": "Joe_Sky <hitomi_doax@sina.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "download-git-repo": "^1.0.1",
    "git-exec": "^0.2.1",
    "global-modules": "^1.0.0",
    "inquirer": "^5.0.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "nornj": "^0.4.5",
    "ora": "^1.3.0",
    "process.args": "0.0.6",
    "targz": "^1.0.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0"
  }
}
