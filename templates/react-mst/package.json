{
  "name": "nornj-react-mst-boilerplate",
  "version": "1.0.0",
  "description": "NornJ + React + Mobx-state-tree boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "node -v | npm run server | cross-env Project=web webpack-dev-server --host 0.0.0.0 --inline --hot --progress",
    "dev-remote": "cross-env Remote=true npm run dev",
    "build": "cross-env Project=web NODE_ENV=production webpack --progress --hide-modules",
    "build-test": "cross-env Project=web BABEL_ENV=development NODE_ENV=test webpack --progress --hide-modules",
    "build-local": "cross-env Project=local NODE_ENV=production webpack --progress --hide-modules",
    "server": "nodemon ./server/app.js",
    "lint": "eslint src",
    "test": "jest --config=test/jest.config.json",
    "coverage": "jest --coverage --config=test/jest.config.json",
    "test:e2e": "jest --config=test/jest.config.e2e.json",
    "e2e": "node ./test/run-e2e.js"
  },
  "author": "joe_sky",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.0.0",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/echarts": "^4.1.6",
    "@types/lodash": "^4.14.133",
    "@types/react": "^16.8.19",
    "@types/react-addons-css-transition-group": "^15.0.5",
    "@types/react-dom": "^16.8.4",
    "@types/rx": "^4.1.1",
    "@types/styled-components": "^4.1.16",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.10.0",
    "babel-plugin-nornj-in-jsx": "^5.0.0-rc.24",
    "babel-plugin-nornj-loader": "^1.0.13",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "core-decorators": "^0.14.0",
    "core-js": "^2.5.1",
    "create-react-class": "^15.6.2",
    "cross-env": "^1.0.8",
    "cross-port-killer": "^1.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^3.2.0",
    {{%jest%}}"less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "0.4.1",
    "multer": "^1.3.0",
    "node-sass": "^4.7.2",
    "nodemon": "1.12.5",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-pxtorem": "^4.0.1",
    "postcss-smart-import": "^0.7.2",
    "precss": "^1.4.0",
    "prettier": "^1.13.5",
    "prettier-eslint": "^8.8.1",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.6.0",
    {{%puppeteer%}}"react-hot-loader": "^3.1.2",
    "redux-devtools": "^3.3.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.0",
    "styled-components": "^4.1.2",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-postcss": "^0.1.3",
    "styled-jsx-plugin-sass": "^0.2.4",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylus-loader": "^1.4.2",
    "tcp-port-used": "^1.0.0",
    "template-html-loader": "^1.0.0",
    "ts-loader": "^2.0.2",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.7",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-hot-middleware": "^2.17.1"
  },
  "dependencies": {
    "antd": "^3.19.2",
    "array-from": "^2.1.1",
    "axios": "^0.18.0",
    "bundle-loader": "^0.5.5",
    "console-polyfill": "^0.3.0",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.15-beta.0",
    "element-react": "^1.4.10",
    "element-theme-default": "^1.4.13",
    "es6-promise": "^4.1.0",
    "es6-weak-map": "^2.0.2",
    "fullpage-react": "^2.0.2",
    "fxios": "^2.0.11",
    "isomorphic-fetch": "^2.2.1",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react": "^6.0.2",
    "mobx-react-lite": "^1.4.0",
    "mobx-state-tree": "^3.14.0",
    "nornj": "^5.0.0-rc.12",
    "nornj-loader": "^5.0.0-rc.2",
    "nornj-react": "^5.0.0-rc.13",
    "react": "^16.8.6",
    "react-burger-menu": "^2.1.3",
    "react-d3-tree": "^1.3.2",
    "react-dom": "^16.8.6",
    "react-mixin": "^2.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-transition": "^1.3.0",
    "universal-cookie": "^2.0.8",
    "whatwg-fetch": "^0.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,less,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "server/app.js",
      "server/routes/**/*.js",
      "server/common/**/*.js"
    ]
  },
  "njCliConfig": {
    "templateType": "single-page",
    "jest": {{%jest ? 'true' : 'false'%}},
    "e2e": {{%puppeteer ? 'true' : 'false'%}},
    "pageTemplates": [
      "default",
      "chart",
      "form",
      {
        "name": "form-ts",
        "ts": true
      },
      {
        "name": "form",
        "alias": "form-mobx",
        "mobx": true
      },
      {
        "name": "form-ts",
        "alias": "form-mobx-ts",
        "mobx": true,
        "ts": true
      },
      "empty",
      {
        "name": "empty-ts",
        "ts": true
      },
      {
        "name": "empty",
        "alias": "empty-mobx",
        "mobx": true
      },
      {
        "name": "empty-ts",
        "alias": "empty-mobx-ts",
        "mobx": true,
        "ts": true
      }
    ],
    "componentTemplates": [
      "default",
      {
        "name": "default",
        "alias": "function",
        "isFunc": true
      }
    ],
    "storeTemplates": [
      "default",
      {
        "name": "default",
        "alias": "mobx",
        "mobx": true
      }
    ]
  }
}
