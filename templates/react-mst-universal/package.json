    "watch": [
      "server/app.js",
      "server/routes/**/*.js",
      "server/common/**/*.js"
    ]
  },
  "njCliConfig": {
    "templateType": "single-page",
    "pageTemplates": [
      "default",
      "chart",
      "form",
      "empty",
      "default-nornj",
      "chart-nornj",
      "form-nornj",
      "empty-nornj"
    ],
    "componentTemplates": [
      "default",
      "default-nornj"
    ],
    "storeTemplates": [
      "default",
      "default-fetch"
    ]
  }
